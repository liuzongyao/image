{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "$pv_name"
  },
  "spec": {
    "capacity": {
      "storage": "$sizeG"
    },
    "accessModes": [
      "ReadWriteMany"
    ],
    "csi": {
      "driver": "fake",
      "volumeHandle": "1",
      "readOnly": false
    }
  }
}
