{
  "resource": {
    "name": "$newapp_name",
    "namespace": "$K8S_NAMESPACE",
    "owners": [
      {
        "name": "$newapp_name",
        "phone": "15211111111"
      }
    ],
    "displayName": "$newapp_name"
  },
  "kubernetes": [
    {
      "apiVersion": "extensions/v1beta1",
      "kind": "Deployment",
      "metadata": {
        "name": "$newapp_name",
        "namespace": "$K8S_NAMESPACE"
      },
      "spec": {
        "replicas": 1,
        "template": {
          "metadata": {
            "labels": {
              "service.alauda.io/name": "$newapp_name"
            }
          },
          "spec": {
            "containers": [
              {
                "image": "$IMAGE",
                "name": "containersname",
                "ports": [
                  {
                    "containerPort": 80,
                    "name": "portsname"
                  }
                ],
                "resources": {
                  "limits": {
                    "cpu": "20m",
                    "memory": "50M"
                  },
                  "requests": {
                    "cpu": "20m",
                    "memory": "50M"
                  }
                }
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "metadata": {
        "name": "$newapp_name",
        "annotations": {
          "loadbalancer.alauda.io/bind": "[]"
        },
        "namespace": "$K8S_NAMESPACE"
      },
      "spec": {
        "selector": {
          "service.alauda.io/name": "deployment-$newapp_name",
          "app.alauda.io/name": "$newapp_name.$K8S_NAMESPACE"
        },
        "ports": [
          {
            "port": 80,
            "targetPort": 80,
            "name": "80-80",
            "protocol": "TCP"
          }
        ],
        "type": "ClusterIP"
      }
    }
  ]
}