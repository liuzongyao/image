{
  "auto_tag_type": "TIME",
  "image_repo": {
    "project": {
      "name": ""
    },
    "name": "$REPO_NAME",
    "registry": {
      "name": "$REGISTRY"
    }
  },
  "name": "$NAME",
  "endpoint_id": "$BUILD_ENDPOINT_ID",
  "auto_build_enabled": false,
  "customize_tag": "latest",
  "space_name": "$SPACE_NAME",
  "namespace": "$NAMESPACE",
  "image_cache_enabled": true,
  "allow_artifact_upload_fail": true,
  "artifact_upload_enabled": true,
  "build_image_enabled": true,
  "ci_enabled": true,
  "ci_config_file_location": "/",
  "code_repo": {
    "code_repo_password": "$SVN_PASSWORD",
    "dockerfile_location": "/",
    "code_repo_username": "$SVN_USERNAME",
    "code_repo_client": "SIMPLE_SVN",
    "code_repo_type": "DIR",
    "build_context_path": "/",
    "code_repo_path": "$SVN_REPO",
    "code_repo_type_value": "/"
  },
  "memory": 512,
  "cpu": 0.25,
  "notification": {
    "name": "$NOTI_NAME",
    "uuid": "$NOTI_UUID"
  }
